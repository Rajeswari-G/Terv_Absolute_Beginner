Input: 
       5 5 4 5
Output: 4


#include<stdio.h>
#include<stdlib.h>
int main()
{
int *a,n,i,j,temp;
scanf("%d",&n);
a=(int*)malloc(sizeof(int)*n);
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
for(i=0;i<n;i++)
{
  for(j=0;j<n;j++)
  {
    if(a[i]>a[j])
    {
      temp=a[i];
      a[i]=a[j];
      a[j]=temp;
    }
   }
 }
 for(i=0;i<n;i++)
 {
    if(a[i]!=a[i+1]
    {
         printf("%d",a[i+1]);
          break;
    }
 }
 return 0;
 }
